<p id="notice"><%= notice %></p>

<h1>Places</h1>

<table>
  <thead>
    <tr>
      <th>Listing title</th>
      <th>Listing image</th>
      <th>Description</th>
      <th>Address</th>
      <th>Price</th>
      <th>Max guest</th>
      <th>Number rooms</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @places.each do |place| %>
      <tr>
        <td><%= place.listing_title %></td>
        <td><%= place.listing_image %></td>
        <td><%= place.description %></td>
        <td><%= place.address %></td>
        <td><%= place.price %></td>
        <td><%= place.max_guest %></td>
        <td><%= place.number_rooms %></td>
        <td><%= place.latitude %></td>
        <td><%= place.longitude %></td>
        <td><%= place.user %></td>
        <td><%= link_to 'Show', place %></td>
        <td><%= link_to 'Edit', edit_place_path(place) %></td>
        <td><%= link_to 'Destroy', place, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Place', new_place_path %>
