<%= form_for([@place, @place.reviews.new]) do |f| %>

  <div id="user_stars">

  <div class="form-group">
    
  </div>
    <%= f.text_area :comment, rows: 3, class: "form-control" %>
  </div>
  

  <%= f.hidden_field :place_id, value: @place.id %>

  <div class="actions">
    <%= f.submit "Create", class: "btn btn-primary" %>
  </div>

<% end %>

<script>
$('#user_stars').raty({
  path: '/assets',
  scoreName: 'review[star]',
  score: 1
});
</script>